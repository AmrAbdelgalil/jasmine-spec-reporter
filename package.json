{
  "name": "jasmine-spec-reporter",
  "version": "2.7.0",
  "description": "Spec reporter for jasmine behavior-driven development framework",
  "main": "dist/jasmine-spec-reporter.js",
  "scripts": {
    "tsc": "rm -rf dist/* && node node_modules/typescript/bin/tsc",
    "test": "npm run tsc && node node_modules/jasmine/bin/jasmine.js",
    "coverage": "npm run tsc && rm -rf coverage && node node_modules/istanbul/lib/cli.js cover node_modules/jasmine/bin/jasmine.js",
    "open-coverage": "npm run coverage && open ./coverage/lcov-report/index.html",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "example": "npm run tsc && node example/run-example.js",
    "webdriver-update": "node node_modules/protractor/bin/webdriver-manager update",
    "webdriver-start": "node node_modules/protractor/bin/webdriver-manager start",
    "protractor-example": "npm run tsc && node node_modules/protractor/bin/protractor example/protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bcaudan/jasmine-spec-reporter"
  },
  "keywords": [
    "jasmine",
    "reporter",
    "bdd",
    "spec",
    "protractor"
  ],
  "author": "Bastien Caudan",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bcaudan/jasmine-spec-reporter/issues"
  },
  "homepage": "https://github.com/bcaudan/jasmine-spec-reporter",
  "dependencies": {
    "colors": "1.1.2"
  },
  "devDependencies": {
    "coffee-script": "1.10.0",
    "coveralls": "2.11.12",
    "istanbul": "0.4.4",
    "jasmine": "2.4.1",
    "jasmine-core": "2.4.1",
    "protractor": "4.0.3",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^1.5.0",
    "typescript": "^1.8.10"
  }
}
